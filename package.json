{
  "name": "spotifyradio",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "live-reload": "nodemon server/index.js",
    "test": "LOG_DISABLED=true NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --no-cache",
    "test:watch": "LOG_DISABLED=true  npm test -- --watchAll",
    "test:coverage": "LOG_DISABLED=true  npm test -- --coverage tests/unit",
    "test:e2e": "LOG_DISABLED=true  npm test -- tests/e2e",

    "live-reload:docker": "docker-compose up --build live-reload",
    "test:docker": "docker-compose up --build test",
    "test:watch:docker": "docker-compose up --build test-watch",
    "test:coverage:docker": "docker-compose up --build test-coverage"
  },
  "engines": {
    "node": "17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yants95/jsexpert-week-6.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yants95/jsexpert-week-6/issues"
  },
  "homepage": "https://github.com/yants95/jsexpert-week-6#readme",
  "devDependencies": {
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "portfinder": "^1.0.28",
    "supertest": "^6.2.2"
  },
  "dependencies": {
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3",
    "throttle": "^1.0.3"
  }
}
